{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% if collector.displayInWdt %}
        {% set icon %}
        <img height="28" alt="Composer Packages" src="data:image/png;base64,"/>
        <span class="sf-toolbar-info-piece-additional-detail">Composer packages</span>
        {% endset %}
        {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Composer packages</b>
            <span>{{ collector.packagecount }}</span>
        </div>
        {% endset %}
        {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
    {% endif %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">
            <img width=30 src="data:image/png;base64," />
        </span>
        <strong>Composer</strong>
        <span class="count">
            <span>{{ collector.packagecount }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    {% if collector.packages is empty %}
        <em>No packages information</em>
    {% else %}
        <h2>Composer Packages:</h2>
        <table>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
            {% for package_name, package in collector.packages %}
                <tr class="{{ loop.index is odd ? 'odd' : 'even' }}">
                    <th><code>{{ package_name }}</code></th>
                    <td>
                        <table>
                            <tr>
                                <td width="80px">version</td>
                                <td>{{ package.version }}</td>
                            </tr>
                            <tr>
                                <td width="80px">source</td>
                                <td>{{ package.source }}</td>
                            </tr>
                            {% if package.authors is defined %}
                            <tr>
                                <td width="80px">authors</td>
                                <td>
                                    <ul>
                                        {% for author in package.authors %}
                                            <li style="padding-bottom: 5px;">{{ author.name }}</li>
                                        {% endfor %}
                                    </ul>
                                </td>
                            </tr>
                            {% endif %}
                            {% if package.homepage is defined %}
                            <tr>
                                <td width="80px">homepage</td>
                                <td>{{ package.homepage }}</td>
                            </tr>
                            {% endif %}
                            {% if package.requires|length > 0 %}
                            <tr>
                                <td width="80px">requires</td>
                                <td>
                                    <ul>
                                        {% for require_name, require_version in package.requires %}
                                            <li style="padding-bottom: 5px;">{{ require_name }}: {{ require_version }}</li>
                                        {% endfor %}
                                    </ul>
                                </td>
                            </tr>
                            {% endif %}
                        </table>
                    </td>
                </tr>
            {% endfor %}
        </table>
    {% endif %}
{% endblock %}